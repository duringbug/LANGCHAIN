{
  "name": "langchain_project",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"vite\"",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "build-backend": "babel src/backend --out-dir backend --minified && find backend -type f -name \"*.js\" -exec bash -c 'mv \"$0\" \"${0%.js}.cjs\"' {} \\;",
    "type-check": "vue-tsc --build",
    "test": "jest",
    "backend": "nodemon src/backend/server.cjs",
    "crawler": "node src/backend/scripts/crawler.cjs",
    "docker-up": "docker-compose up -d",
    "docker-down": "docker-compose down",
    "initialize": "node src/backend/scripts/initialize-cloud.cjs"
  },
  "dependencies": {
    "vue": "^3.5.13",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/preset-env": "^7.26.0",
    "@langchain/cohere": "^0.3.1",
    "@langchain/core": "^0.3.26",
    "@langchain/openai": "^0.3.16",
    "@tsconfig/node22": "^22.0.0",
    "@types/axios": "^0.9.36",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.9",
    "babel-preset-minify": "^0.5.2",
    "chromedriver": "^131.0.4",
    "cli-progress": "^3.12.0",
    "cohere-ai": "^7.15.0",
    "concurrently": "^9.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jest": "^29.7.0",
    "langchain": "^0.3.7",
    "marked": "^15.0.6",
    "mysql2": "^3.11.5",
    "naive-ui": "^2.40.4",
    "nodemon": "^3.1.9",
    "npm-run-all2": "^7.0.1",
    "postcss": "^8.4.49",
    "selenium-webdriver": "^4.27.0",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3",
    "urllib": "^4.6.11",
    "uuid": "^11.0.3",
    "vfonts": "^0.0.3",
    "vite": "^6.0.1",
    "vite-plugin-vue-devtools": "^7.6.5",
